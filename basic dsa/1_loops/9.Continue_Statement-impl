public class Main {
    public static void main(String[] args)
    { System.out.println("Printing 0 to 5 ");
      for(int i=0;i<=5;i++)
      {
          if(i==3)
          {System.out.println("--Skipping iteration for 3--");
              continue;
          }
          System.out.println(i);
       }
        System.out.println();
        System.out.println("out of loop");
    }
}