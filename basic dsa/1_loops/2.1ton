// "static void main" must be defined in a public class.

public class Main {
    public static void main(String[] args) {
        // print 1ton,n given by user.
        Scanner sc=new Scanner(System.in);
        int range=sc.nextInt();
        int counter=1;
        while(counter<= range){
            System.out.print(counter+" ");
            counter++;
        }
    }
}