//In Java, you can call one constructor from another 
//constructor within the same class using the this() keyword.
//This is known as constructor chaining.
public class Main {
    private int x;
    private int y;
    public Main (int x)
    {
        this(x,0);
    }
    public Main(int x,int y)
    {
        this.x=x;
        this.y=y;
    }
    public void printvalues(){
        System.out.println("x= "+ x+" " +"y= "+y);
    }
    public static void main(String[] args) {
        Main obj1 =new Main(5);
        obj1.printvalues();
        Main obj2 =new Main(10,20);
        obj2.printvalues();   
    }
} 
//x= 5 y= 0
//x= 10 y= 20